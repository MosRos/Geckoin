<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuItemView">

        <attr name="android:fontFamily" />

        <attr name="startIcon" format="reference"/>
        <attr name="showStartIcon" format="boolean"/>
        <attr name="title" format="string"/>
        <attr name="subtitle" format="string"/>
        <attr name="showSubtitle" format="boolean"/>
        <attr name="description" format="string"/>
        <attr name="showDescription" format="boolean"/>
        <attr name="endIcon" format="reference"/>
        <attr name="showEndIcon" format="boolean"/>
        <attr name="showBottomDivider" format="boolean"/>
        <attr name="titleTextAppearance" format="reference"/>
        <attr name="subtitleTextAppearance" format="reference"/>
    </declare-styleable>
</resources>